msgstr "Plugin-Name"

#: ../C/brasero.xml:1684(entry)
msgid "Description"
msgstr "Beschreibung"

#: ../C/brasero.xml:1689(guilabel)
msgid "File checksum"
msgstr "File checksum"

#: ../C/brasero.xml:1690(entry)
#, fuzzy
msgid ""
